<div class="ccntainer-fluid">
	<div class="row">
		<div class="col-lg-6 col-md-6 col-sm-6">
			<div class="card card-stats">
				<div class="card-header" data-background-color="green">
					<i class="material-icons">attach_money</i>
				</div>
				<div class="card-content">
					<p class="category">Income</p>
					<h3 class="title">$34,500</h3>
				</div>
				<div class="card-footer">
					<div class="stats">
						<i class="material-icons">date_range</i> Last 30 Days
					</div>
				</div>
			</div>
		</div>
		<div class="col-lg-6 col-md-6 col-sm-6">
			<div class="card card-stats">
				<div class="card-header" data-background-color="blue">
					<i class="material-icons">group</i>
				</div>
				<div class="card-content">
					<p class="category">Active Students</p>
					<h3 class="title">250</h3>
				</div>
				<div class="card-footer">
					<div class="stats">
						<i class="material-icons">person</i> Updated <%= Time.now.strftime("%m/%d/%Y") %>
					</div>
				</div>
			</div>
		</div>
	</div>

	<div class="row">
		<div class="col-md-6">
			<div class="card">
				<div class="card-header card-chart" data-background-color="purple">
					<div class="ct-chart" id=""></div>
				</div>
				<div class="card-content">
					<h4 class="title">Attendance</h4>
					<p class="category"><span class="text-success"><i class="fa fa-long-arrow-up"></i> 55%  </span> Last 30 Days</p>
				</div>
				<div class="card-footer">
					<div class="stats">
						<i class="material-icons">access_time</i> updated 4 minutes ago
					</div>
				</div>
			</div>
		</div>
		<div class="col-md-6">
			<div class="card">
				<div class="card-header card-chart" data-background-color="purple">
					<div class="ct-chart" id=""></div>
				</div>
				<div class="card-content">
					<h4 class="title">Growth</h4>
					<p class="category"><span class="text-success"><i class="fa fa-long-arrow-up"></i> 55%  </span> Last 30 Days</p>
				</div>
				<div class="card-footer">
					<div class="stats">
						<i class="material-icons">access_time</i> updated 4 minutes ago
					</div>
				</div>
			</div>
		</div>
	</div>

</div>
